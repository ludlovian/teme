{
  "name": "teme",
  "version": "1.1.0",
  "description": "Stream/observables",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"|snazzy",
    "format": "prettier-standard \"src/**/*.js\" \"test/**/*.js\"",
    "build:dev": "npm run lint && microbundle --no-compress",
    "build:watch": "npm run lint && microbundle watch --no-compress",
    "build:prod": "npm run lint && microbundle",
    "test": "npm run lint && nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && http-server coverage/ -p 8000"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^1.2.1",
    "esm": "^3.2.5",
    "http-server": "^0.11.1",
    "microbundle": "^0.9.0",
    "nyc": "^13.3.0",
    "prettier-standard": "^9.1.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {},
  "ava": {
    "require": [
      "esm"
    ]
  }
}
